<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="dao.sw_cultureMapper" >
  <resultMap id="BaseResultMap" type="pojo.sw_culture" >
    <id column="SouthwestVillageID" property="southwestvillageid" jdbcType="INTEGER" />
  </resultMap>
  <resultMap id="ResultMapWithBLOBs" type="pojo.sw_culture" extends="BaseResultMap" >
    <result column="DwellingsTypes" property="dwellingstypes" jdbcType="LONGVARCHAR" />
    <result column="FolkCulture" property="folkculture" jdbcType="LONGVARCHAR" />
    <result column="DwellingsChange" property="dwellingschange" jdbcType="LONGVARCHAR" />
    <result column="SocialSpace" property="socialspace" jdbcType="LONGVARCHAR" />
    <result column="SacredSpace" property="sacredspace" jdbcType="LONGVARCHAR" />
  </resultMap>
  <sql id="Base_Column_List" >
    SouthwestVillageID
  </sql>
  <sql id="Blob_Column_List" >
    DwellingsTypes, FolkCulture, DwellingsChange, SocialSpace, SacredSpace
  </sql>
  <select id="getAllCulture"  resultType="com.muc_village.pojo.sw_cultureInfo">
    SELECT * FROM southwest_culture
  </select>

  <select id="getContent" parameterType="int" resultType="com.muc_village.pojo.sw_cultureInfo">
    select * from southwest_culture
    where SouthwestVillageID=#{SouthwestVillageID}
  </select>

</mapper>